{
    "name": "gos/react-amqp",
    "description": "AMQP bindings for ReactPHP",
    "keywords": ["amqp", "rabbitmq"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Cook",
            "email": "jeremycook0@gmail.com"
        },
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-amqp": "*",
        "evenement/evenement": "^3.0",
        "react/event-loop": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15.1",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": { "Gos\\Component\\ReactAMQP\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gos\\Component\\ReactAMQP\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
