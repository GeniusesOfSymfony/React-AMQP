{
    "name": "gos/react-amqp",
    "description": "AMQP bindings for ReactPHP",
    "keywords": ["amqp", "rabbitmq"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Cook",
            "email": "jeremycook0@gmail.com"
        },
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-amqp": "*",
        "evenement/evenement": "~3.0.1",
        "react/event-loop": "~0.5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6"
    },
    "autoload": {
        "psr-4": { "Gos\\Component\\ReactAMQP\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gos\\Component\\ReactAMQP\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
